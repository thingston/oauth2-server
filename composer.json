{
    "name": "thingston/oauth2-server",
    "type": "library",
    "description": "Secure OAuth2 Server library conforming with RFC 6749.",
    "keywords": ["php", "oauth2"],
    "homepage": "https://thingston.com",
    "license": "MIT",
    "authors": [
        {"name": "Pedro Ferreira", "email": "pedro@thingston.com"}
    ],
    "require": {
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Thingston\\": "src/Thingston/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThingstonTest\\": "test/"
        }
    }
}
